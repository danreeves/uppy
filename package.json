{
  "name": "uptime-checker",
  "version": "1.0.0",
  "description": "Website uptime checker using Cloudflare Workers",
  "main": "src/index.js",
  "scripts": {
    "dev": "npx wrangler dev",
    "deploy": "node deploy.js",
    "deploy:direct": "npx wrangler deploy",
    "setup": "node setup.js",
    "setup-auth": "node setup-auth.js",
    "kv:create": "npx wrangler kv:namespace create UPTIME_KV",
    "kv:create-preview": "npx wrangler kv:namespace create UPTIME_KV --preview",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [
    "cloudflare",
    "workers",
    "uptime",
    "monitoring"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "prettier": "^3.5.3",
    "wrangler": "^3.0.0"
  }
}
